
@{
    ViewBag.Title = "UploadFile";
}
<h2>Subir Imagen</h2>

    <form class="col-xs-12" method="post" action="SubirImagen" enctype="multipart/form-data">
        <div class="form-group">
        <p>Rut del usuario:</p>
         <input type="text" class="form-control" name="Rut" />
         </div>
         <br />
        <div class="form-group">
            <input type="file" class="form-control" name="image" />
        </div>
        <br />
        <div class="form-group">
            <button type="submit" class="btn btn-primary col-xs-12">Subir</button>
        </div>
    </form>
