@{
    ViewData["Title"] = "Home Page";
}
<div class="text-center">
    <h1 class="display-4">Welcome</h1>  
    <h3>Ingrese un RUT</h3>
    <form method="GET" asp-action="Modificar">
        <input type="text" placeholder="Rut" name="Rut" value="@ViewData["Search"]" id="Rut" />
        <input type="submit" value="Modificar" class="btn btn-default" />
    </form>
    <form method="GET" asp-action="Eliminar">
        <input type="text" placeholder="Rut" name="Rut" value="@ViewData["Search"]" id="Rut" />
        <input type="submit" value="Eliminar" class="btn btn-default" />
    </form>
   
  
    <script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>
</div>
